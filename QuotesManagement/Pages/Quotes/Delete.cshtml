@page "{quoteId}"
@model QuotesManagement.Pages.Quotes.DeleteModel
@{ }

<h2>Delete</h2>

<div class="alert alert-danger">
    Are you sure you want to delete @Model.Quote.Title?
</div>

<form method="Post">
    <button type="submit" class="btn btn-danger">Yes</button>
    <a asp-page="List" class="btn btn-default">Cancel</a>
</form>
